# Contributor: haru <haru@nak.ics.keio.ac.jp>

pkgname=lua-tokyocabinet
pkgver=1.10
pkgrel=1
pkgdesc="lua API for TokyoCabinet"
arch=('i686')
url="http://www.1978th.net/tokyocabinet/"
license=('custom')
depends=('lua')
makedepends=()
optdepends=()
source=(http://www.1978th.net/tokyocabinet/luapkg/tokyocabinet-lua-1.10.tar.gz)
md5sums=('e5f0b19ed2f559899edb804054552c40')

build() {
  cd "$srcdir"
  tar xf tokyocabinet-lua-1.10.tar.gz
  cd tokyocabinet-lua-1.10
  ./configure
  make
  make DESTDIR="${pkgdir}" install
}
